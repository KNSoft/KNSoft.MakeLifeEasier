<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
  <metadata>
    <id>KNSoft.MakeLifeEasier</id>
    <version>1.0.0-beta</version>
    <title>KNSoft.MakeLifeEasier</title>
    <authors>KNSoft</authors>
    <projectUrl>https://github.com/KNSoft/KNSoft.MakeLifeEasier</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <readme>README.md</readme>
    <description>A fast, low-level and convenient C/C++ library to "Make Life Easier" when suffering from Windows NT development.</description>
    <tags>Windows Win32 WinSDK</tags>
    <repository type="git" url="https://github.com/KNSoft/KNSoft.MakeLifeEasier" />
  </metadata>
  <files>
    <file src="..\README.md" target="" />
    <file src="KNSoft.MakeLifeEasier.targets" target="build\" />

    <file src="KNSoft.MakeLifeEasier\**\*.h" exclude="KNSoft.MakeLifeEasier\**\*.g.h;Resource.h;Resource.Embedded.h" target="build\Include\KNSoft\MakeLifeEasier\" />

    <file src="OutDir\**\*.lib" exclude="OutDir\x86\**" target="build\Lib\" />
    <file src="OutDir\**\*.pdb" exclude="OutDir\x86\**;OutDir\**\Test.pdb" target="build\Lib\" />
    <file src="OutDir\x86\**\*.lib" target="build\Lib\Win32\" />
    <file src="OutDir\x86\**\*.pdb" exclude="OutDir\x86\**\Test.pdb" target="build\Lib\Win32\" />
  </files>
</package>